/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // making sure the comments are ok
    Not(in=a,out=outa);
    Not(in=b,out=outb);
    And(a=outa,b=b,out=outc);
    And(a=a,b=outb,out=outd);
    Or(a=outc,b=outd,out=out);
    
}
